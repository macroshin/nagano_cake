<h3>注文履歴一覧</h3>

  <table>
    <thead>
      <tr>
        <th>購入日時</th>
        <th>購入者</th>
        <th>注文個数</th>
        <th>注文ステータス</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <% @orders.zip(@order_details).each do |order order_detail| %>
        <td><%= order.created_at %></td>
        <td><%= order.customer.last_name %><%= order.customer.first_name %></td>
        <td></td>
      </tr>
    </tbody>
  </table>